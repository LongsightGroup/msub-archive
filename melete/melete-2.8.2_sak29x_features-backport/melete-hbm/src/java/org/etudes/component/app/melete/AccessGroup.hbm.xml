<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class 
      name="org.etudes.component.app.melete.AccessGroup"
      table="melete_access_group">
      <id name="accessId" column="ACCESS_ID" unsaved-value="null">
      	<generator class="hilo">
      		<param name="sequence">MELETE_ACCESS_GROUP_S</param>
      	</generator>
      </id> 
		
		<many-to-one name="module" 
		class="org.etudes.component.app.melete.Module" 
		insert="false" update="false" column="MODULE_ID" lazy="false"/>	      

	    <property name="groupId">
	      <column name="GROUP_ID" length="255" not-null="true" />
	    </property>
    </class>
    
</hibernate-mapping>
