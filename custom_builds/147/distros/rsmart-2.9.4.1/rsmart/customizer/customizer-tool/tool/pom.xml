                </includes>
                <filtering>true</filtering>
                <targetPath>${project.build.directory}/${project.artifactId}-${version}/WEB-INF</targetPath>
            </resource-->
          </resources>
          <plugins>
            <plugin>
              <artifactId>maven-war-plugin</artifactId>
              <configuration>
                <webXml>${project.build.directory}/web.xml</webXml>
              </configuration>
            </plugin>
          </plugins>
        </build>
    </profile>
