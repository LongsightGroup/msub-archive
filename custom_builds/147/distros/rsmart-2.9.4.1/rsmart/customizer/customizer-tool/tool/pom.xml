  <build>
    <resources>
      <resource>
        <directory>src/bundle</directory>
        <includes>
          <include>**/*.properties</include>
        </includes>
      </resource>
      <resource>
        <directory>src/java</directory>
        <includes>
          <include>**/*.hbm.xml</include>
        </includes>
      </resource>
    </resources>
<!-- java source -->
    <sourceDirectory>src/java</sourceDirectory>
    <testSourceDirectory>src/test/java</testSourceDirectory>
    <testResources>
      <testResource>
        <directory>src/webapp</directory>
        <includes>
          <include>**/*.xml</include>
          <include>**/*.xsd</include>
          <include>**/*.properties</include>
        </includes>
      </testResource>
<!--includes:  include: **/*Test.java -->
    </testResources>
  </build>
  <profiles>
    <profile>
      <id>osiv</id>
      <activation>
        <property>
          <name>rsmart-customizer-tool.osiv</name>
        </property>
      </activation>
        <build>
          <filters>
            <filter>../../../../../config/maven_profiles/${project.artifactId}/osiv.properties</filter>
          </filters>
          <resources>
            <resource>
                <directory>src/webapp/WEB-INF</directory>
                <includes>
                    <include>web.xml</include>
                </includes>
                <filtering>true</filtering>
                <targetPath>${project.build.directory}</targetPath>
            </resource>
            <!--resource>
                <directory>../../../../../config/maven_profiles/${project.artifactId}/resources</directory>
                <includes>
